<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.club.mapper.FavoriteClubMapper">
    <select id="selectByFavoriteId" resultType="com.example.club.entity.FavoriteClub">
        select *
        from favorite_club
        where favorite_id = #{id}
    </select>

    <select id="selectByUserId" resultType="com.example.club.entity.FavoriteClub">
        select *
        from favorite_club
        where user_id = #{userId}
    </select>
</mapper>